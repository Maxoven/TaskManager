# ВАЖНО! Скопируй этот файл в .env и заполни своими данными

# MySQL Database
DB_HOST=localhost
DB_USER=твой_пользователь_mysql
DB_PASSWORD=твой_пароль
DB_NAME=твоя_база_данных

# JWT Secret (сгенерируй случайную строку минимум 32 символа)
JWT_SECRET=твой-супер-секретный-ключ-минимум-32-символа

# Server Port
PORT=3001

# Environment
NODE_ENV=production
